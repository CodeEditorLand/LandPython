{
	"name": "@vscode/python-extension",
	"version": "0.0.1",
	"private": "false",
	"description": "An API facade for the Python extension in VS Code",
	"homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Nikola Hristov",
		"email": "nikola@nikolahristov.tech",
		"url": "https://nikolahristov.tech"
	},
	"publisher": "playform",
	"type": "module",
	"main": "./out/main.js",
	"types": "./out/main.d.ts",
	"scripts": {
		"all": "npm run clean && npm run compile",
		"all:publish": "git clean -xfd . && npm install && npm run compile && npm run formatTypings",
		"clean": "node ../node_modules/rimraf/bin.js out",
		"compile": "node ./node_modules/typescript/lib/tsc.js -b ./tsconfig.json",
		"formatTypings": "node ../node_modules/eslint/bin/eslint.js --fix ./out/main.d.ts",
		"lint": "node ../node_modules/eslint/bin/eslint.js --ext ts src",
		"prepack": "npm run all:publish",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"devDependencies": {
		"@types/vscode": "1.84.2",
		"source-map": "0.8.0-beta.0",
		"typescript": "5.3.2",
		"typescript-esbuild": "0.3.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
